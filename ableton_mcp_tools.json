        {
            "name": "get_song_info",
            "description": "Get current song information including tempo, time signature, and playback state",
            "input_schema": {
                "type": "object",
                "properties": {},
                "required": []
            },
            "osc_mapping": {
                "type": "composite",
                "calls": [
                    "/live/song/get/tempo",
                    "/live/song/get/signature_numerator",
                    "/live/song/get/signature_denominator",
                    "/live/song/get/is_playing",
                    "/live/song/get/current_song_time",
                    "/live/song/get/loop_start",
                    "/live/song/get/loop_length"
                ]
            }
        },
